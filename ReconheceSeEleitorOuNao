#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
	int idade;
	printf("Qual sua idade?\n");
	scanf("%d", &idade);
	system("cls");
	if(idade<16){
		printf("N%co eleitor.",198);
	}
	else if(idade>=16 && idade<18 ||idade>=70){
		printf("Eleitor facultativo.");
	}
	else if(idade>=18 && idade<70){
		printf("Eleitor obrigat%crio.",162);
	}	
	
	return 0;
}
